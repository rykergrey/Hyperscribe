{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next",
        "config": {
          "eslint": {
            "ignoreDuringBuilds": true
          },
          "typescript": {
            "ignoreBuildErrors": true
          }
        }
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin"
          },
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "require-corp"
          }
        ]
      }
    ]
  }